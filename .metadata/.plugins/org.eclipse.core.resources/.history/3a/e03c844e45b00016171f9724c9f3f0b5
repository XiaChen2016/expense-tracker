package com.tracker.repositories.Projects;

import org.springframework.data.domain.Page;
import org.springframework.data.repository.PagingAndSortingRepository;

import com.tracker.domain.receipt.Receipt;

public interface MongoProjectRepository extends UpdateableProjectRepository ,
												PagingAndSortingRepository< Receipt,String > {

	public Page<Receipt> findByOwnerId( String id );
}
